void UPLINK_SUCCESS_CHECK()
{
   if(UPLINK_SUCCESS == 0)
   {
      UPLINK_SUCCESS = 1;
      AUTO_CAM_DONE = 1;
      FIRST_HSSC_DONE = 1;
      AUTO_LDM_DONE = 1;
      AUTO_ADCS_DONE = 1;
      STORE_FLAG_INFO();
      WRITE_FLAG_to_EEPROM();
   }
   return;
}


void EXECUTE_MISSION_from_COM(int8 CMD1,int8 CMD2,int8 CMD3,int8 CMD4,int8 CMD5)
{
   if(CMD1 != 0x50)
   {
      UPLINK_SUCCESS_CHECK();
      ACK_for_COM[14] = 0x00;
   }
   switch(CMD1)
   {
      case 0xC0:
         REPLY_TO_COM(0x66,0);
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         
         CAM_TEST_OPERATION(CMD2);
         STORE_ADRESS_DATA_TO_FLASH();
         break;
      case 0xA0:
         REPLY_TO_COM(0x66,0);
         SAVE_SAT_LOG(CMD1,0,CMD2);
         unsigned int16 duration = (unsigned int16)CMD2*60;
         
         ADCS_TEST_OPERATION(duration);
         STORE_ADRESS_DATA_TO_FLASH();
         break;
      case 0xD0:
         REPLY_TO_COM(0x66,0);
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         
         DATACOLLECTION_TEST_OPERATION();
         STORE_ADRESS_DATA_TO_FLASH();
         delay_ms(10);
         break;
      case 0x99:
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         CURRENT_ADDRESS_OF_FLASH();
         break;
      case 0xaa:
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0x22:
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         RESET_SATELLITE();
         break;
      case 0xb0:
         CLEAR_BC_FLAG();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb1:
         MAKE_BC_FLAG_1();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb2:
         MAKE_BC_FLAG_2();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb3:
         MAKE_BC_FLAG_3();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb4:
         MAKE_BC_FLAG_4();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0x4b:
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         Count_Kill_Flag();
         break;
      case 0xbb:
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         Disable_Kill();
         break;
      case 0xfa:
         REPLY_TO_COM(0x66,0);
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         HIGHSAMP_SENSOR_COLLECTION(HIGH_SAMP_TIMES);
         STORE_ADRESS_DATA_TO_FLASH();
         break;
      case 0x59:
         REPLY_TO_COM(0x66,0);
         GIVE_ACCESS_SCF_Nsec(CMD2);
         break;
      case 0x88:
         CURRENT_FLAG_STATUS();
         break;
      case 0x9f:
         Disp_RSV();
         break;
      case 0x50:
         CW_RESPOND();
         break;
      case 0xe0:
         REFRESH_SECTOR_OF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0xe1:
         REFRESH_SECTOR_SCF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0xe2:
         REFRESH_SECTOR_SMF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x12:
         TRANSFER_SECTOR_OF2SCF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x32:
         TRANSFER_SECTOR_SMF2SCF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x21:
         TRANSFER_SECTOR_SCF2OF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x7f:
         SAVE_SAT_LOG(0x7f, 0, 0);
         SEND_CMD_FOR_RESET_SATELLITE();//forced reset
         break;
   }
   return;
}











void EXECUTE_MISSION_from_PC(int8 CMD1,int8 CMD2,int8 CMD3,int8 CMD4,int8 CMD5)
{
   if(CMD1 != 0x50)
   {
      UPLINK_SUCCESS_CHECK();
      ACK_for_COM[14] = 0x00;
   }
   switch(CMD1)
   {
      case 0xC0:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         CAM_TEST_OPERATION(CMD2);
         STORE_ADRESS_DATA_TO_FLASH();
         UPLINK_SUCCESS_CHECK();
         break;
      case 0xA0:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         unsigned int16 duration = (unsigned int16)CMD2*60;
         UPLINK_SUCCESS_CHECK();
         ADCS_TEST_OPERATION(duration);
         break;
      case 0xD0:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         DATACOLLECTION_TEST_OPERATION();
         delay_ms(10);
         break;
      case 0x99:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         CURRENT_ADDRESS_OF_FLASH();
         break;
      case 0xaa:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0x22:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         RESET_SATELLITE();
         break;
      case 0xb0:
         CLEAR_BC_FLAG();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb1:
         MAKE_BC_FLAG_1();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb2:
         MAKE_BC_FLAG_2();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;               
      case 0xb3:
         MAKE_BC_FLAG_3();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0xb4:
         MAKE_BC_FLAG_4();
         STORE_ADRESS_DATA_TO_FLASH();
         STORE_FLAG_INFO();
         WRITE_FLAG_to_EEPROM();
         break;
      case 0x4b:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         Count_Kill_Flag();
         break;
      case 0xbb:
         SAVE_SAT_LOG(CMD1, 0, CMD2);
         Disable_Kill();
         break;
      case 0xfa:
         SAVE_SAT_LOG(CMD1,0,CMD2);
         HIGHSAMP_SENSOR_COLLECTION(HIGH_SAMP_TIMES);
         Turn_OFF_ADCS();
         break;
      case 0x59:
         GIVE_ACCESS_SCF_Nsec(CMD2);
         break;
      case 0xee:
         ERASE_EEPROM_INFO();
         MEMORY_ERASE();
         break;
      case 0x88:
         CURRENT_FLAG_STATUS();
         break;
      case 0x9f:
         Disp_RSV();
         break;
      case 0xcf:
         table_refresh();
         break;
      case 0x6a:
         RESET_FLAG_DATA();
         break;
      case 0xe0:
         REFRESH_SECTOR_OF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0xe1:
         REFRESH_SECTOR_SCF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0xe2:
         REFRESH_SECTOR_SMF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x12:
         TRANSFER_SECTOR_OF2SCF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x32:
         TRANSFER_SECTOR_SMF2SCF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x21:
         TRANSFER_SECTOR_SCF2OF(CMD2,CMD3,CMD4,CMD5);//this function contains saving log
         break;
      case 0x7f:
         SAVE_SAT_LOG(0x7f, 0, 0);
         SEND_CMD_FOR_RESET_SATELLITE();//forced reset
         break;
   }
   return;
}



int8 CHECK_MEMORY_FUNCTION(int8 data)
{
   if((data == 0xe0)||(data == 0xe1)||(data == 0xe2))
   {
      data = 0;
   }
   if((data == 0x12)||(data == 0x32)||(data == 0x21))
   {
      data = 0;
   }
   return data;
}
